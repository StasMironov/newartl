	"contacts_form": {
		"layout": "2",
		"id": "form-1",
		"name": "form-1",
		"method": "POST",
		"action": "/ajax/contacts/?repo=artel.frontend&status=success",
		"attr": "data-ajax-form data-scroll-target data-scroll-contacts",
		"title": "Связаться с нами",
		"bg": "green",
		"fields": [
			{
				"input": true,
				"id": "form-1-input-1",
				"name": "form-1-input-1",
				"theme": "white",
				"required": true,
				"label": "ФИО"
			},
			{
				"input": true,
				"id": "form-1-input-2",
				"name": "form-1-input-2",
				"theme": "white",
				"required": true,
				"label": "Компания"
			},
			{
				"input": true,
				"id": "form-1-input-3",
				"name": "form-1-input-3",
				"theme": "white",
				"required": true,
				"type": "tel",
				"label": "Телефон",
				"mask_list": [
					"+998-##-###-####",
					"+7(###)###-##-##",
					"+380(##)###-##-##",
					"+7(6##)###-##-##",
					"+992-##-###-####",
					"+994-##-###-##-##",
					"+996-###-######"
				]
			},
			{
				"textarea": true,
				"id": "form-1-textarea-1",
				"name": "form-1-textarea-1",
				"theme": "white",
				"label": "Сообщение"
			}
		],
		"agreement": [
			{
				"checkbox": true,
				"id": "form-1-checkbox-1",
				"name": "form-1-checkbox-1",
				"text": "Настоящим даю согласие по установленной форме на обработку моих <a href='javascript:void(0);' class='link-content' target='_blank'>персональных данных</a> в порядке, установленном законом РУз от 27.07.2006 N 152-ФЗ 'О персональных данных'. Код подтверждения",
				"required": true,
				"theme": "white"
			}
		],
		"submit": {
			"type": "submit",
			"text": "Отправить",
			"title": "Отправить",
			"theme": "white",
			"icon": {
				"name": "24/arrow-up-right"
			}
		}
	},
